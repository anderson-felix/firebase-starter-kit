    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "8.5.5",
    "@types/mime-types": "2.1.1",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "babel-plugin-module-resolver": "4.1.0",

    "firebase": "^10.12.1",
    "firebase-admin": "^12.1.1",
    "fireorm": "^0.23.3",
    "joi": "^17.10.2",
    "jsonwebtoken": "8.5.1",
    "mime-types": "2.1.33",
    "pino": "7.2.0",
    "pino-http": "^8.5.0",
    "pino-pretty": "7.2.0",
